{
  "name": "krzysztofslonina/laravel-woocommerce-sync-bridge",
  "description": "Laravel app that syncs WooCommerce products, orders, and customers into local DB.",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "laravel/framework": "^11.0",
    "laravel/tinker": "^2.9"
  },
  "require-dev": {
    "fakerphp/faker": "^1.23",
    "nunomaduro/collision": "^8.0",
    "phpunit/phpunit": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "@php artisan package:discover --ansi"
    ],
    "test": "@php artisan test"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
